compilers:
- compiler:
    spec: clang@15
    paths:
      cc: /usr/lib64/distcc/clang
      cxx: /usr/lib64/distcc/clang++
      f77: /bin/gfortran
      fc: /bin/gfortran
    flags:
      cflags: -fPIE
      cxxflags: -fPIE
      fflags: -fallow-argument-mismatch
    operating_system: mageia9
    target: x86_64
    modules: []
    environment: {}
    extra_rpaths: []
