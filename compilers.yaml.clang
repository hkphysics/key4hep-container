compilers:
- compiler:
    spec: clang@17
    paths:
      cc: /usr/lib64/distcc/clang
      cxx: /usr/lib64/distcc/clang++
      f77: /bin/gfortran
      fc: /bin/gfortran
    flags:
      fflags: -fallow-argument-mismatch
    operating_system: mageia10
    target: x86_64
    modules: []
    environment: {}
    extra_rpaths: []
