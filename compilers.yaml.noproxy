compilers:
- compiler:
    spec: gcc@11.2.1
    paths:
      cc: /bin/gcc
      cxx: /bin/g++
      f77: /bin/gfortran
      fc: /bin/gfortran
    flags:
      fflags: -fallow-argument-mismatch
      cxxflags: -I/usr/include/c++/10
    operating_system: mageia9
    target: x86_64
    modules: []
    environment: {}
    extra_rpaths: []
