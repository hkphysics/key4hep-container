compilers:
- compiler:
    spec: gcc@14.2.0
    paths:
      cc: /bin/gcc
      cxx: /bin/g++
      f77: /bin/gfortran
      fc: /bin/gfortran
    flags:
      fflags: -fallow-argument-mismatch
      cxxflags: -std=c++20
    operating_system: mageia10
    target: x86_64
    modules: []
    environment: {}
    extra_rpaths: []
