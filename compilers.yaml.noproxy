compilers:
- compiler:
    spec: gcc@12.2.1
    paths:
      cc: /bin/gcc
      cxx: /bin/g++
      f77: /bin/gfortran
      fc: /bin/gfortran
    flags:
      fflags: -fallow-argument-mismatch
      cxxflags: -I/usr/include/c++/12 -std=c++17
    operating_system: mageia9
    target: broadwell
    modules: []
    environment: {}
    extra_rpaths: []
