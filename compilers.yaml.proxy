compilers:
- compiler:
    spec: gcc@11.1.1
    paths:
      cc: /usr/lib64/distcc/gcc
      cxx: /usr/lib64/distcc/g++
      f77: /bin/gfortran
      fc: /bin/gfortran
    flags:
      fflags: -fallow-argument-mismatch
      cxxflags: -I/usr/include/c++/11
    operating_system: mageia9
    target: x86_64
    modules: []
    environment: {}
    extra_rpaths: []
